{
  "name": "World-Cleaner",
  "version": "1.0.0",
  "description": "World Wide Competition for Image propose only",
  "main": "src/app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "nodemon",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odicforcesounds/World-Cleaner.git"
  },
  "keywords": [
    "brands",
    "image",
    "competition",
    "india",
    "Europe",
    "USA",
    "America",
    "Asia",
    "Africa",
    "business",
    "world Wide"
  ],
  "homepage": "https://github.com/odicforcesounds/World-Cleaner#readme",
  "author": "OscarFM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/odicforcesounds/World-Cleaner/issues"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "firebase-functions-test": "^0.2.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "firebase": "^8.2.7",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "views"
    ]
  },
  "firebase": {
    "database": {
      "rules": "database.rules.json"
    },
    "firestore": {
      "rules": "firestore.rules",
      "indexes": "firestore.indexes.json"
    },
    "hosting": {
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "./node_modules/**"
      ]
    },
    "storage": {
      "rules": "storage.rules"
    },
    "function": {
      "predeploy": [
        "npm ../ --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/{css,js}/*.{css.js,map}",
    "js/{src,dist}/**/*.{js.css}"
  ],
  "funding": {
    "type": "open"
  }
}
